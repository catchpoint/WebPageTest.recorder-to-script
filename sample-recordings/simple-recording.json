{
  "title": "My site",
  "steps": [
    {
      "type": "setViewport",
      "width": 1705,
      "height": 600,
      "deviceScaleFactor": 1,
      "isMobile": false,
      "hasTouch": false,
      "isLandscape": false
    },
    {
      "type": "navigate",
      "url": "https://timkadlec.com/",
      "assertedEvents": [
        {
          "type": "navigation",
          "url": "https://timkadlec.com/",
          "title": "TimKadlec.com - Web Performance Consulting | TimKadlec.com"
        }
      ]
    },
    {
      "type": "click",
      "selectors": [
        [
          "aria/WORK WITH ME"
        ],
        [
          "#nav > ul > li:nth-child(2) > a"
        ]
      ],
      "target": "main",
      "offsetX": 66,
      "offsetY": 30,
      "assertedEvents": [
        {
          "type": "navigation",
          "url": "https://timkadlec.com/me/",
          "title": "Work With Me - Web Performance Consulting | TimKadlec.com"
        }
      ]
    },
    {
      "type": "click",
      "selectors": [
        [
          "aria/WRITING"
        ],
        [
          "#nav > ul > li:nth-child(3) > a"
        ]
      ],
      "target": "main",
      "offsetX": 32.40625,
      "offsetY": 31,
      "assertedEvents": [
        {
          "type": "navigation",
          "url": "https://timkadlec.com/remembers/",
          "title": "Writing - Web Performance Consulting | TimKadlec.com"
        }
      ]
    },
    {
      "type": "click",
      "selectors": [
        [
          "aria/SPEAKING"
        ],
        [
          "#nav > ul > li:nth-child(4) > a"
        ]
      ],
      "target": "main",
      "offsetX": 46.609375,
      "offsetY": 46,
      "assertedEvents": [
        {
          "type": "navigation",
          "url": "https://timkadlec.com/talks/",
          "title": "Speaking - Web Performance Consulting | TimKadlec.com"
        }
      ]
    }
  ]
}